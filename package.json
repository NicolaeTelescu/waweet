{
  "name": "what-are-we-eating-today",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-p startMongoDB startNode",
    "startNode": "node app.js",
    "startDev": "run-p startSASS startReact startMongoDB startNodemon",
    "startNodemon": "nodemon app.js",
    "startReact": "npx babel --watch src/js --out-dir ./public/javascripts",
    "startMongoDB": "mongod --dbpath=\"Database/Database/\" --logpath=\"Database/Database/logs/logs.txt\"",
    "startSASS": "node-sass -q --watch --output-style compressed src/stylesheets/main.scss public/stylesheets/style.css",
    "seedDB": "npm run createFilesDB && (run-p startMongoDB mongoSeed)",
    "mongoSeed": "node ./Database/seeding/index.js",
    "createFilesDB": "node ./Database/createFiles.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "css-loader": "^5.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.2.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "http-errors": "~1.6.3",
    "jimp": "^0.16.1",
    "mongo-seeding": "^3.5.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.11",
    "morgan": "~1.9.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "promise": "^8.1.0",
    "pug": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "serve-favicon": "^2.5.0",
    "style-loader": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7"
  }
}
